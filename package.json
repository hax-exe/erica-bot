{
    "name": "erica-bot",
    "version": "1.0.0",
    "description": "Erica Bot - A high-end multi-purpose Discord bot with moderation, music, leveling, economy, and more",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc && cp -r src/assets dist/",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "dev:once": "tsx src/index.ts",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "deploy": "tsx src/deploy-commands.ts",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "keywords": [
        "discord",
        "bot",
        "moderation",
        "music",
        "leveling",
        "economy"
    ],
    "author": "",
    "license": "UNLICENSED",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.962.0",
        "@discordjs/rest": "^2.4.0",
        "@napi-rs/canvas": "^0.1.88",
        "cors": "^2.8.5",
        "discord.js": "^14.16.3",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.45.1",
        "express": "^5.2.1",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.2",
        "kazagumo": "^3.2.0",
        "pg": "^8.13.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.0.0",
        "shoukaku": "^4.1.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^25.0.3",
        "@types/pg": "^8.16.0",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "@vitest/coverage-v8": "^4.0.16",
        "drizzle-kit": "^0.31.8",
        "esbuild": "^0.25.0",
        "eslint": "^9.39.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.50.0",
        "vitest": "^4.0.16"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "overrides": {
        "esbuild": "^0.25.0"
    }
}
